{
  "name": "advertisement-microservice",
  "version": "0.0.1",
  "description": "基于web3钱包的广告投放系统",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "cross-env ENV_VALUE=local nest start --watch",
    "debug": "nest start --debug --watch",
    "build": "rimraf dist && nest build",
    "docker:build:local": "docker build --rm -f dockerfile.local -t advertisement-microservice-local .",
    "start:local": "cross-env ENV_VALUE=local pm2-runtime ./dist/main.js",
    "docker:build:test": "docker build --rm -f dockerfile.test -t advertisement-microservice-test .",
    "docker:push:test": "docker tag advertisement-microservice-test 18058173171/advertisement-microservice-test:latest && docker push 18058173171/advertisement-microservice-test:latest",
    "start:test": "cross-env ENV_VALUE=test pm2-runtime ./dist/main.js",
    "docker:build:prod": "docker build --rm -f dockerfile.prod -t advertisement-microservice-prod .",
    "docker:push:prod": "docker tag advertisement-microservice-prod 18058173171/advertisement-microservice-prod:latest && docker push 18058173171/advertisement-microservice-prod:latest",
    "start:prod": "cross-env ENV_VALUE=prod pm2-runtime ./dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.118.1",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/typeorm": "^8.1.4",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "ioredis": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/cache-manager": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/pbkdf2": "^3.1.0",
    "@types/secp256k1": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  }
}
